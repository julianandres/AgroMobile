<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_process"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.julian.agromobile.ProcessActivity">

    <TextView
        android:id="@+id/name_process"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/txt_nombre"
        android:textColor="#000000"
        android:textSize="25sp"/>
    <TextView
        android:id="@+id/state_process"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:text="@string/txt_estado"
        android:textSize="20sp"/>
    <TextView
        android:id="@+id/date_init_process"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:text="@string/txt_fecha_inicio"
        android:textSize="20sp"/>
    <TextView
        android:id="@+id/date_fin_process"
        android:layout_width="match_parent"
        android:textColor="#000000"
        android:layout_height="wrap_content"
        android:text="@string/txt_fecha_fin"
        android:textSize="20sp"/>
    <ListView
        android:id="@+id/listSubProcess"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        ></ListView>
</LinearLayout>
